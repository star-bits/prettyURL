{
    "manifest_version": 3,
    "name": "prettyURL",
    "version": "1.0",
    "permissions": ["activeTab", "clipboardWrite"],
    "action": {
      "default_icon": {
        "48": "icons/icon_48.png"
      },
      "default_popup": ""
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icons/icon_16.png",
      "48": "icons/icon_48.png",
      "128": "icons/icon_128.png"
    }
  }
  